.result_container {
    margin-top: 40rpx;
    border-right:1px solid #bdbdbd;
    border-left:1px solid #bdbdbd;
    border-top:1px solid #bdbdbd;
  }
  .tcell {
    padding: 16rpx;
    line-height: 2.2;
    text-align: left;
    border-bottom: 1px solid #bdbdbd;
    vertical-align: top;
    display: table-cell;
    font-size: 16px;
    width: 350rpx;
    overflow: hidden;
    white-space: nowrap;
  }
  .th {
    font-weight: bold;
  }
  .container{
    width: 90%;
    margin-left: 5%;
  }
  .picker {
    margin-top:30rpx;
    text-align:center;
    margin-bottom:20rpx;
    font-size: 20px;
  }
  .tcell_container {
    display: flex;
  }
  .image{
    width: 30px;
    height: 30px;
    float: right;
  }
  .more_result {
    float: right;
    font-size: 14px;
    color:#999;
  }
